{
    "presets": [["@babel/preset-env", { "modules": false }]],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-arrow-functions",
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "h",
                "pragmaFrag": "Fragment"
            }
        ]
    ]
}
